Xft.dpi: 96
Xft.antailias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.lcdfilter: lcdfilder

XTerm*renderFont: true
XTerm*faceName: xft:Hack Nerd Font
XTerm*utf8: 2 
XTerm*locale: true
XTerm.vt100.locale: false
XTerm.vt100.utf8: true
XTerm*faceSize: 10

XTerm*faceSize1: 8
XTerm*faceSize2: 10
XTerm*faceSize3: 12
XTerm*faceSize4: 16
XTerm*faceSize5: 22
XTerm*faceSize6: 24

XTerm*vt100.scrollbar: false
XTerm*scrollTtyOutput: false
XTerm*saveLines: 100

XTerm*highlightSelection: true
XTerm*trimSelection: true

XTerm*loginshell: true

XTerm*charClass:33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,95:48,126:48

XTerm.vt100.metaSendsEscape: true
XTerm.vt100.translations: #override \n\
  Ctrl <Key> -: smaller-vt-font() \n\
  Ctrl <Key> +: larger-vt-font() \n\
  Ctrl <Key> 0: set-vt-font() \n\
  Ctrl Shift <Key> C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key> V: insert-selection(CLIPBOARD)

UXTerm*renderFont: true
UXTerm*faceName: xft:Hack Nerd Font
UXTerm*utf8: 2
UXTerm*locale: true
UXTerm.vt100.locale: false
UXTerm.vt100.utf8: true
UXTerm*faceSize: 12

UXTerm*faceSize1: 10
UXTerm*faceSize2: 12
UXTerm*faceSize3: 16
UXTerm*faceSize4: 22
UXTerm*faceSize5: 24
UXTerm*faceSize6: 28

UXTerm*vt100.scrollbar: false
UXTerm*scrollTtyOutput: false
UXTerm*saveLines: 100

UXTerm*highlightSelection: true
UXTerm*trimSelection: true

UXTerm*loginshell: true

UXTerm*charClass:33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,95:48,126:48

UXTerm.vt100.metaSendsEscape: true
UXTerm.vt100.translations: #override \n\
  Ctrl <Key> -: smaller-vt-font() \n\
  Ctrl <Key> +: larger-vt-font() \n\
  Ctrl <Key> 0: set-vt-font() \n\
  Ctrl Shift <Key> C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key> V: insert-selection(CLIPBOARD)

